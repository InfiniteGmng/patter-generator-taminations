<tamination title="Squeeze">

  <tam title="Ends Squeeze"
       from="Columns"
       formation="Column RH GBGB">
    <path numbers="8 1">
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY=".5" />
    </path>
    <path numbers="6 3">
    </path>
    <path numbers="4 5">
    </path>
    <path numbers="2 7">
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY=".5" />
    </path>
  </tam>

  <tam title="Ends Squeeze"
       from="Butterfly"
       formation="Butterfly RH">
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
    </path>
    <path>
    </path>
  </tam>

  <tam title="Squeeze"
       from="Butterfly"
       formation="Butterfly RH">
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY=".5" />
    </path>
  </tam>

  <tam title="Squeeze" from="Diamonds" formation="Diamonds RH Girl Points" fractions="">
    <path>
      <move select="Swing Right" />
      <move select="Dodge Left" beats="1" scaleY=".25"/>
    </path>
    <path>
      <move select="Dodge Right"/>
      <move select="Swing Right" hands="gripright" scaleX=".5"/>
    </path>
    <path>
      <move select="Swing Right" />
      <move select="Dodge Left" beats="1" scaleY=".25"/>
    </path>
    <path>
      <move select="Dodge Right"/>
      <move select="Swing Right" hands="gripright" scaleX=".5"/>
    </path>
  </tam>

  <tam title="Squeeze"
       from="O"
       formation="O RH">
    <path>
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
  </tam>

  <tam title="Centers Tandem Squeeze"
       from="Right-Hand Columns" formation="Column RH GBGB">
    <path>
    </path>
    <path>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Forward 2" beats="1.5"/>
      <move select="Quarter Right" beats="1.5" offsetX="2"/>
    </path>
    <path>
    </path>
  </tam>

  <tam title="Centers Tandem Squeeze"
       from="Left-Hand Columns" formation="Column LH GBGB">
    <path>
    </path>
    <path>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Forward 2" beats="1.5"/>
      <move select="Quarter Left" beats="1.5" offsetX="2"/>
    </path>
    <path>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Lead Left" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <path>
    </path>
  </tam>

  <tam title="Squeeze the Hourglass"
       from="Right-Hand Hourglass"
       formation="Hourglass RH BP" >
    <path numbers="3 6">
      <move select="Quarter Right" beats="3" offsetY="-3" />
      <move select="Hinge Right" hands="gripright" />
    </path>
    <path numbers="1 8">
      <move select="Stand" hands="nohands" beats="4.5" />
    </path>
    <path numbers="2 7">
      <move select="Lead Right" beats="3" scaleY="3" />
      <move select="Quarter Right" hands="gripright" offsetX="1" />
    </path>
    <path numbers="4 5">
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" beats="3" scaleX="3.1" scaleY=".5" />
    </path>
  </tam>

  <tam title="Squeeze the Hourglass"
       from="Left-Hand Hourglass"
       formation="Hourglass LH BP" >
    <path>
      <move select="Lead Left" beats="3" scaleY="3" />
      <move select="Quarter Left" hands="gripleft" offsetX="1" />
    </path>
    <path>
      <move select="Stand" hands="nohands" beats="4.5" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetY="3" />
      <move select="Hinge Left" hands="gripleft" />
    </path>
    <path>
      <move select="Hinge Left" scaleX=".5" />
      <move select="Lead Left" beats="3" scaleX="3.1" scaleY=".5" />
    </path>
  </tam>

  <tam title="Squeeze the Galaxy"
       from="Right-Hand Galaxy"
       formation="Galaxy RH GP">
    <path numbers="2 7">
      <move select="Hinge Right" />
      <move select="Quarter Right" beats="3" offsetX="3" />
    </path>
    <path numbers="1 8">
      <move select="Stand" hands="nohands" beats="4.5" />
    </path>
    <path numbers="3 6">
      <move select="Quarter Right" hands="right" offsetY="-1" />
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
    <path numbers="5 4">
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
  </tam>

  <tam title="Squeeze the Galaxy"
       from="Left-Hand Galaxy"
       formation="Galaxy LH GP">
    <path>
      <move select="Quarter Left" hands="left" offsetY="1" />
      <move select="Lead Left" beats="3" scaleX="3" />
    </path>
    <path>
      <move select="Stand" hands="nohands" beats="4.5" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Quarter Left" beats="3" offsetX="3" />
    </path>
    <path>
      <move select="Lead Left" beats="3" scaleX=".5" scaleY="3" />
      <move select="Hinge Left" scaleY=".5" />
    </path>
  </tam>

</tamination>
